<div class="panel">
  <div class="panel-body">
    <div class='row'>
      <h1 class="page-title js-name" style="text-align:center; font-size: 200px;"><%= radical.name %></h1>
      <h1 class="page-title js-name" style="text-align:center"><%= radical.china %></h1>
      <h3 class="page-title" style="text-align:center"><%= radical.mean %></h3>
      <div class="col-sm-4">

      </div>
      <div class="form-group col-sm-4">
        <input type="text" class="form-control input-lg js-input" style="text-align: center" placeholder="<%= radical.name %>">
      </div>
      <div class="col-sm-4">
        <h4 class="page-title js-times" style="
          height: 50px;
          width: 50px;
          border-radius: 50%;
          border: 1px solid;
          text-align: center;
          padding: 13px;">
            <%= radical.times || 0 %>
        </h4>
      </div>
      <div class="form-group col-sm-12">
        <button type="button" class="btn btn-primary"> Back </button>
      </div>
    </div>
  </div>
</div>